apiVersion: v1
kind: Service
metadata:
   name: hamm-cost-updater
spec:
  ports:
    - name: http
      protocol: TCP
      port: 80
      targetPort: 80
    - name: https
      protocol: TCP
      port: 443
      targetPort: 443
  type: NodePort
  selector:
    service: hamm-cost-updater
    environment: {{env "ENVIRONMENT"}}
    project: hamm-cost-updater
